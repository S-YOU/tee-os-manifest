    <linkfile src="kbuild-tool" dest="tools/kbuild"/>
    <linkfile src="elfloader-tool" dest="tools/elfloader" />
</project>
<project name="musllibc.git" path="libs/libmuslc" revision="sel4"/>
<project name="seL4_libs.git" path="projects/seL4_libs">
